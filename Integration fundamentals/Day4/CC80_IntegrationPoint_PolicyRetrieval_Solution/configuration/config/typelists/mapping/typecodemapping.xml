<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!--
    Mapping for sample Homeowners PAS integration to ExternalApp
-->
<typecodemapping>
    
	<namespacelist>
		<namespace name="acme:pas" />
	</namespacelist>

	<typelist name="PolicyType">
		<mapping typecode="BusinessAuto" namespace="acme:pas" alias="CommercialAuto"/>
		<mapping typecode="PersonalAuto" namespace="acme:pas" alias="PersonalAuto"/>
		<mapping typecode="Homeowners"   namespace="acme:pas" alias="Homeowner's"/>
	</typelist>
    <typelist name="CoverageType">
        <mapping typecode="HOLI_Personal_Liability_HOE" namespace="acme:pas" alias="Homeowner's Personal Liability"/>	
		<mapping typecode="HOLI_Med_Pay_HOE"            namespace="acme:pas" alias="Homeowner's Medical Payments"/>	
		<mapping typecode="HODW_Dwelling_Cov_HOE"       namespace="acme:pas" alias="Homeowner's Dwelling"/>	
		<mapping typecode="HODW_Loss_Of_Use_HOE"        namespace="acme:pas" alias="Homeowner's Loss Of Use"/>	
		<mapping typecode="HODW_Earthquake_HOE"         namespace="acme:pas" alias="Earthquake Coverage"/>	
		<mapping typecode="HODW_Personal_Property_HOE"  namespace="acme:pas" alias="Homeowner's Personal Property"/>	
		<mapping typecode="HODW_FungiCov_HOE"           namespace="acme:pas" alias="Fungus Coverage"  />	
        <mapping typecode="HODW_Other_Structures_HOE"   namespace="acme:pas" alias="Homeowner's Other Structures"/>	
        <!-- START: Typecodes added for policy retrieval lab -->		
		<mapping typecode="PALiabilityCov"     namespace="acme:pas" alias="Liability - Bodily Injury and Property Damage"/>			
		<mapping typecode="PAUMBICov"          namespace="acme:pas" alias="Uninsured Motorist - Bodily Injury"/>			
		<mapping typecode="PAUMPDCov"          namespace="acme:pas" alias="Uninsured Motorist - Property Damage"/>			
		<mapping typecode="PACollisionCov"     namespace="acme:pas" alias="Collision"/>			
		<mapping typecode="PAComprehensiveCov" namespace="acme:pas" alias="Comprehensive"/>			
		<mapping typecode="PATowingLaborCov"   namespace="acme:pas" alias="Towing"/>	
        <!-- END: Typecodes added for policy retrieval lab -->			
    </typelist>
	
</typecodemapping>
